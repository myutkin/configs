Xcursor.theme: Bluecurve 
Xcursor.size: 32

URxvt*geometry:			100x25
urxvt*background:  		rgba:0000/0000/0000/eeee
URxvt*foreground:       	#aaaaaa
URxvt*inheritPixmap:    	false ! to disable pseudo-transp
URxvt*reverseVideo:		false
!URxvt*tintColor:        	#dfdfdf
URxvt*shading:	        50		
URxvt*fading:	        	70		!term unfocus      
URxvt*font:	     		xft:monospace:size=11
URxvt*scrollBar:		false
!URxvt*transparent: false
!URxvt*borderLess: true
urxvt*internalBorder: 4
urxvt.secondaryScroll: false
!set to 32-bit for real transparency (compositing required)
urxvt*depth:             32
!tint background with this color
urxvt*tintColor:          black
!! BEGIN urlLauncher settings !!
urxvt*perl-lib:           /usr/lib/urxvt/perl/
urxvt*perl-ext-common:    default,matcher
urxvt*urlLauncher:        /usr/bin/firefox
urxvt*matcher.button:     1
!! END urlLauncher settings !!

!transparent=0000 opaque=ffff
!urxvt*background: rgba:1111/1111/1111/dddd

Xft.dpi:96
Xft.hinting:true
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hintstyle:hintslight

*headingFont:   xft:monospace:size=11 
